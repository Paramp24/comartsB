{
    "version": 2,
    "builds": [
      { "src": "mysite/wsgi.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "mysite.settings",
      "DATABASE_URL": "postgres://user:password@host:port/dbname"
    }
  }
  
